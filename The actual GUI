loadstring(game:HttpGet("https://raw.githubusercontent.com/Simon2928/Roblox-LUA-Repository/main/Music%20additions", true))()
wait(0.1)
local username = game:GetService("Players").LocalPlayer.Name
local Win11Lib = loadstring(game:HttpGet("https://raw.githubusercontent.com/Skribb11es/Win11UILib/main/main.lua", true))()

local menu = Win11Lib.createMenu("Paranormal UI", Enum.KeyCode.RightShift, true, 1200, 600, "", username, "UI lib by Skribb11es")

-----------------------------------------------------------------------

local misc = menu.addTab("Misc", "MiscFlag", true, "7059346386")
misc.addButton("Mouse Overlay", "Just provides and extra mouse if you can't see it while in the menu", function() loadstring(game:HttpGet("https://raw.githubusercontent.com/Simon2928/Roblox-LUA-Repository/main/extra%20mouse", true))() end)

local dropdownmisc = misc.addDropdown("Music!", "Contains local music!")

dropdownmisc.addOption("Spooky Scary Skeletons", "SpookyFlag", false, function(ex) if ex == true then
    game.Workspace["Spooky"].Playing = true
    game.Workspace["Spooky"].Looped = true
elseif ex == false then
    game.Workspace["Spooky"].Playing = false
    game.Workspace["Spooky"].Looped = false
    game.Workspace["Spooky"].TimePosition = 0
    end
end)

dropdownmisc.addOption("Mama Mia", "MamaFlag", false, function(ex) if ex == true then
    game.Workspace["MamaMia"].Playing = true
    game.Workspace["MamaMia"].Looped = true
elseif ex == false then
    game.Workspace["MamaMia"].Playing = false
    game.Workspace["MamaMia"].Looped = false
    game.Workspace["MamaMia"].TimePosition = 0
    end
end)

dropdownmisc.addOption("Blue Sky (Slow Jazz Remix)", "BlueSkyFlag", false, function(ex) if ex == true then
    game.Workspace["BlueSky"].Playing = true
    game.Workspace["BlueSky"].Looped = true
elseif ex == false then
    game.Workspace["BlueSky"].Playing = false
    game.Workspace["BlueSky"].Looped = false
    game.Workspace["BlueSky"].TimePosition = 0
    end
end)

dropdownmisc.addOption("Happy Remix", "HappyFlag", false, function(ex) if ex == true then
    game.Workspace["Happy"].Playing = true
    game.Workspace["Happy"].Looped = true
elseif ex == false then
    game.Workspace["Happy"].Playing = false
    game.Workspace["Happy"].Looped = false
    game.Workspace["Happy"].TimePosition = 0
    end
end)

dropdownmisc.addOption("Blinding Lights", "BlindingFlag", false, function(ex) if ex == true then
    game.Workspace["BlindingLights"].Playing = true
    game.Workspace["BlindingLights"].Looped = true
elseif ex == false then
    game.Workspace["BlindingLights"].Playing = false
    game.Workspace["BlindingLights"].Looped = false
    game.Workspace["BlindingLights"].TimePosition = 0
    end
end)




misc.addSlider("Music Volume Changer", "VolumeFlag", 0, 100, 50, true, "Changes the volume of the songs above", "6824925104",
    function(ex) game.Workspace["Spooky"].Volume = ex/100
    game.Workspace["BlindingLights"].Volume = ex/100
    game.Workspace["MamaMia"].Volume = ex/100
    game.Workspace["BlueSky"].Volume = ex/100
    game.Workspace["Happy"].Volume = ex/100
    end)



-----------------------------------------------------------------------

local tab = menu.addTab("Project Shark", "exTab1Flag", true, "6824925104")
tab.addButton("RLGL Teleport", "Teleports you to the red light green light finish, it's an easy game nerd, its also obvious (tps you under doll)", function() loadstring(game:HttpGet("https://raw.githubusercontent.com/Simon2928/Roblox-LUA-Repository/main/ProjectShark%20RLGL%20Teleport", true))() end)
tab.addButton("Honeycomb Finish", "Automatically completes the Honeycomb cutter game", function() loadstring(game:HttpGet("https://raw.githubusercontent.com/Simon2928/Roblox-LUA-Repository/main/Honeycomb%20Auto", true))() end)
tab.addButton("Glass Floor TP", "NOTE: Wait until the cutscene for the specific game is over, then press the button", function() game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(681.067139, 57.3738213, -494.087128) end)
tab.addButton("Squid Game TP", "TP's you to the arena", function() game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-405.848938, 5.921875, 307.155945) end)
tab.addToggle("Fly me to the moon", "exampleToggleFlag", "Game has this audio so why not (Both in lobby and game)", false ,function(ex) if ex == true then
    game.SoundService["Fly me to the moon (Squid Game)"].Playing = true
    game.SoundService["Fly me to the moon (Squid Game)"].Looped = true
elseif ex == false then
    game.SoundService["Fly me to the moon (Squid Game)"].Playing = false
    game.SoundService["Fly me to the moon (Squid Game)"].Looped = false
    game.SoundService["Fly me to the moon (Squid Game)"].TimePosition = 0
    end
    end)
tab.addSlider("Fly me to the moon [Volume]", "exSliderFlag", 0, 100, 50, true, "Changes the volume of the song toggled above", "6824925104", function(ex) game.SoundService["Fly me to the moon (Squid Game)"].Volume = ex/100 end)
tab.addButton("Turn off current theme", "Turns off that DODODODODODOO shit", function() game.ReplicatedStorage["Regular Music"].Volume = 0 end)

-----------------------------------------------------------------------
